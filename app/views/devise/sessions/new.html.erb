<div class='sign-up_log-in-page'>
  <header>
    <h1>ログイン</h1>
  </header>
 <hr>
 <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="field">
      <%= f.label :メールアドレス %><br />
      <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
   </div>

    <div class="field">
      <%= f.label :パスワード %><br />
      <%= f.password_field :password, autocomplete: "current-password" %>
    </div>

    <% if devise_mapping.rememberable? %>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
    <% end %>

    <div class="actions">
      <%= f.submit "ログイン" ,class:"user-btn" %>
    </div>
 <% end %>

<%= render "devise/shared/links" %>

 <%= button_to 'Facebookでログイン', user_facebook_omniauth_authorize_path, method: :post, class:"sns-user-btn" %>
 <%= button_to 'Googleでログイン', user_google_oauth2_omniauth_authorize_path, method: :post, class:"sns-user-btn" %>

</div>
